{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Kafka": {
    "BootstrapServers": "localhost:9092",
    "SecurityProtocol": "PLAINTEXT",
    "ClientId": "wikimedia-reader",
    "Producer": {
      "Acks": "all",
      "EnableIdempotence": true,
      "CompressionType": "snappy",
      "Retries": 3,
      "LingerMs": 5,
      "BatchSize": 32768
    }
  }
}
